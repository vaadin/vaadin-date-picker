{
  "extends": "eslint:recommended",
  "rules": {
    "quotes": ["error", "single"]
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "globals": {
    "vaadin": false,
    "require": false,
    "CustomElements": false,
    "HTMLImports": false,
    "Polymer": false,
    "WeakMap": false
  }
}
